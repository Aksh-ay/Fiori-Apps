<core:FragmentDefinition
    xmlns="sap.ui.comp.valuehelpdialog"
    xmlns:m="sap.m"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core">
    <ValueHelpDialog
        title="Plant"
        ok=".onPlantVHOkPress"
        cancel=".onPlantVHCancelPress"
        supportRanges="false"
        supportMultiselect="false"
        key="Werks"
        descriptionKey="Plant">
        <filterBar>
            <fb:FilterBar advancedMode="true" search=".onPlantVHFilterBarSearch" isRunningInValueHelpDialog="true">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Werks"
                        label="Plant"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Werks" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Sort2"
                        label="Search Term 2"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Sort2" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Sort1"
                        label="Search Term 1"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Sort1" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="PostCode1"
                        label="Postal Code"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="PostCode1" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Name2"
                        label="Name 2"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Name2" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Nation"
                        label="Version"
                        visibleInFilterBar="true">
                        <fb:control>
                            <m:Input name="Nation" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
